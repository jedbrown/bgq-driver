#     watson:    domain is "watson.ibm.com"
#     pok:       domain is "pok.ibm.com"
#     rchland:   domain is "rchland.ibm.com"
#     raleigh:   domain is "raleigh.ibm.com"
#     unknown:   everything else
#
# Action:
#    $bgq_location set to one of ( watson rchland raleigh none )
